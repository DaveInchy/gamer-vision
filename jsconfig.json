{
    "$schema": "https://json.schemastore.org/jsconfig",
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "NodeNext",
        "target": "ES2020",
        "jsx": "react",
        "strictNullChecks": false,
        "strictFunctionTypes": true
    },
    "exclude": [
        "node_modules",
        "**/node_modules/*"
    ]
}